{
  "customDictionary": "sample.dic",
  "lint": {
    "list-item-indent": false,
    "maximum-line-length": false,
    "no-file-name-articles": false,
    "no-file-name-consecutive-dashes": false,
    "no-file-name-mixed-case": false,
    "no-file-name-outer-dashes": false
  },
  "readability": {
    "age": 18,
    "minWords": 7
  },
  "units": [
    "GB",
    "MB",
    "KB",
    "K",
    "am",
    "pm",
    "in",
    "ft"
  ],
  "ignore": [
    "address",
    "attempt",
    "capability",
    "combined",
    "contains",
    "effect",
    "expiration",
    "function",
    "host",
    "hosts",
    "initial",
    "minimize",
    "minimum",
    "multiple",
    "option",
    "previous",
    "request",
    "require",
    "requires",
    "submit",
    "transmit",
    "try",
    "type"
  ],
  "fatal": [
    "blockquote-indentation",
    "checkbox-character-style",
    "checkbox-content-indent",
    "code-block-style",
    "definition-spacing",
    "fenced-code-marker",
    "filler",
    "final-definition",
    "final-newline",
    "first-heading-level",
    "hard-break-spaces",
    "heading-increment",
    "heading-style",
    "link-title-style",
    "no-auto-link-without-protocol",
    "no-duplicate-definitions",
    "no-duplicate-headings",
    "no-emphasis-as-heading",
    "no-file-name-irregular-characters",
    "no-heading-content-indent",
    "no-heading-indent",
    "no-html",
    "no-literal-urls",
    "no-missing-blank-lines",
    "no-multiple-toplevel-headings",
    "no-table-indentation",
    "no-tabs",
    "no-undefined-references",
    "no-unused-definitions",
    "ordered-list-marker-style",
    "ordered-list-marker-value",
    "retext-equality",
    "retext-readability",
    "retext-simplify",
    "rule-style",
    "spelling",
    "table-pipe-alignment",
    "table-pipes",
    "unordered-list-marker-style"
  ]
}
